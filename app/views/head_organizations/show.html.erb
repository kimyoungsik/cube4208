<h1>Head Organization</h1>

<p>
  <b>Name</b><br>
  <%= @head_organization.name %>
</p>

<%= render :partial => 'shared/facebook_like_box', :locals => { :facebook_page => @head_organization.facebook_page } %>

<div class="form-actions">
  <%= link_to 'Back', head_organizations_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_head_organization_path(@head_organization), :class => 'btn' %>
  <%= link_to 'Delete', head_organization_path(@head_organization), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
