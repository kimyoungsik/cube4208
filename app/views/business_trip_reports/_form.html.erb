<%= simple_form_for(@business_trip_report) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :entry_id %>
    <%= f.input :participants %>
    <%= f.input :start %>
    <%= f.input :end %>
    <%= f.input :perpose %>
    <%= f.input :location %>
    <%= f.input :interview %>
    <%= f.input :interviewee %>
    <%= f.input :content %>
    <%= f.input :result %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
