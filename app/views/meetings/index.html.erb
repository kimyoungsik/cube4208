<h1>Listing meetings</h1>

<table>
  <tr>
    <th>Entry</th>
    <th>Participants</th>
    <th>Start</th>
    <th>End</th>
    <th>Perpose</th>
    <th>Location</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @meetings.each do |meeting| %>
  <tr>
    <td><%= meeting.entry_id %></td>
    <td><%= meeting.participants %></td>
    <td><%= meeting.start %></td>
    <td><%= meeting.end %></td>
    <td><%= meeting.perpose %></td>
    <td><%= meeting.location %></td>
    <td><%= meeting.content %></td>
    <td><%= link_to 'Show', meeting %></td>
    <td><%= link_to 'Edit', edit_meeting_path(meeting) %></td>
    <td><%= link_to 'Destroy', meeting, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meeting', new_meeting_path %>
