<%= simple_form_for(@meeting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :entry_id %>
    <%= f.input :participants %>
    <%= f.input :start %>
    <%= f.input :end %>
    <%= f.input :perpose %>
    <%= f.input :location %>
    <%= f.input :content %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
